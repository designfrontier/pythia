{
  "name": "pythia",
  "version": "2.0.0",
  "description": "Outputs the emails for each person with >20% ownership of any file in your commit.",
  "main": "./lib/api",
  "bin": {
    "pythia": "./lib/pythia"
  },
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier .",
    "prettier:write": "prettier --write .",
    "test": "jest",
    "release": "npm test && echo ' => releasing' && tom",
    "release:minor": "npm test && echo ' => releasing' && tom --type=minor",
    "release:major": "npm test && echo ' => releasing' && tom --type=major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/designfrontier/pythia.git"
  },
  "keywords": [
    "bug",
    "bugs",
    "code-review"
  ],
  "author": "Daniel Sellers <daniel@designfrontier.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/designfrontier/pythia/issues"
  },
  "preferGlobal": true,
  "homepage": "https://github.com/designfrontier/pythia#readme",
  "dependencies": {
    "split": "^1.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.14",
    "@swc/jest": "^0.2.23",
    "@types/jest": "29.0.3",
    "@types/node": "18.7.22",
    "@types/split": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "@typescript-eslint/typescript-estree": "^5.38.1",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "6.0.1",
    "eslint-prettier": "0.0.0-empty",
    "husky": "8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "sinon": "^7.4.2",
    "tom-sawyer": "^0.3.0",
    "ts-jest": "29.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint"
  }
}
